{
  "expo": {
    "name": "Lawbot",
    "slug": "Lawbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon4.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "android":{
      "permissions":["READ_EXTERNAL_STORAGE", "INTERNET"],
      "versionCode": 1,
      "package": "lawbotc.kr",
      "icon": "./app/assets/logo4.png",
      "adaptiveIcon": {
        "backgroundColor": "#9E1A1A",
        "foregroundImage": "./app/assets/logo4.png"
      }
    }
  }
}
